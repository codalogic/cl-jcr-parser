uritemplate https://raw.githubusercontent.com/codalogic/dsl-pa/master/${path}${file}

default $h_path include/dsl-pa/
$path include/dsl-pa/
copy dsl-pa.h          ${h_path}
copy dsl-pa-dsl-pa.h   ${h_path}
copy dsl-pa-alphabet.h ${h_path}
copy dsl-pa-reader.h   ${h_path}

default $cpp_path src/dsl-pa/
$path src/
copy dsl-pa-dsl-pa.cpp   ${cpp_path}
copy dsl-pa-alphabet.cpp ${cpp_path}
copy dsl-pa-reader.cpp   ${cpp_path}
