{
   "Image" : {
       $dimensions,
       "Title" : string,
       "Thumbnail" : {
           "Url" : uri,
           $dimensions
       },
       "Animated" : boolean,
       "IDs" : [ $id * ]
   }
}

$dimensions = ( $width, $height )
$width = "Width" : 0..1280
$height = "Height" : 0..1024
$id = integer
