#import org.ietf.geometry as geometry

{
    "Image" : {
        $geometry.dimensions,
        "Title" : string,
        $thumbnail ?,
        "Animated" : boolean,
        "IDs" : [ $id * ]
    }
}

$thumbnail =
    "Thumbnail" : {
        "Url" : uri,
        $geometry.dimensions
    }

$id = integer
